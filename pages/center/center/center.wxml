<view class="page">
  <view class="header">
    <image src="{{userInfo.avatarUrl}}"></image>
    <view class="userInfo">
      <view class='username'>{{userInfo.nickName}}</view>
    </view>
  </view>
  <view class='list'>
    <navigator class='item' hover-class="none" url='../../address/list/list'>我的地址</navigator>
    <view class='item' hover-class="none" bindtap="toOrderList">我的订单</view>
    <view class='item' hover-class="none" bindtap="collection">收藏夹</view>
  </view>
</view>
