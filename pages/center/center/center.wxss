.header{
  background:#f7d373;
  height: 190rpx;
}
.header image{
  width:124rpx;
  height: 124rpx;
  border-radius: 50%;
  display:inline-block;
  margin:20rpx 20rpx 0rpx 24rpx;
  vertical-align:middle;
}
.header .userInfo{
  display:inline-block;
  vertical-align:middle;
}
.username{
  color:#333;
  font-size:32rpx;
}
.list{
  margin-top:20rpx;
  background:#fff;
}
.list .item{
  height: 96rpx;
  line-height: 96rpx;
  color: #333;
  font-size: 32rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
}